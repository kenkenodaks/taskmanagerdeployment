{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url"
  },
  "routes": [
    {
      "src": "^/api/.*",
      "status": 404
    },
    {
      "src": "/",
      "dest": "/index.html",
      "methods": ["GET"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"]
    }
  ]
}
